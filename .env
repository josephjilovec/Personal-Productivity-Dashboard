# API key for Google Cirq/qsim quantum backend
# Obtain from Google Quantum Engine or set to empty for local simulation
CIRQ_API_KEY=<your_cirq_api_key>

# API key for IBM Qiskit quantum backend
# Obtain from IBM Quantum Platform or set to empty for local simulation
QISKIT_API_KEY=<your_qiskit_api_key>

# API key for PennyLane quantum backend
# Obtain from PennyLane provider (e.g., Xanadu Cloud) or set to empty for local simulation
PENNYLANE_API_KEY=<your_pennylane_api_key>

# Port for the REST API server
# Default is 8000 for local development
PORT=8000
